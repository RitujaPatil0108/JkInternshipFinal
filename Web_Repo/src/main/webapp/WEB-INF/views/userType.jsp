<script>
jQuery(document).ready(function($) 
		{
	jQuery(document).on('click', '.iconInner', function(e) {
		jQuery(this).parents('.botIcon').addClass('showBotSubject');
		$("[name='msg']").focus();
	});

	jQuery(document).on('click', '.closeBtn, .chat_close_icon', function(e) {
		jQuery(this).parents('.botIcon').removeClass('showBotSubject');
		jQuery(this).parents('.botIcon').removeClass('showMessenger');
	});

	jQuery(document).on('submit', '#botSubject', function(e) 
			{
		e.preventDefault();

		jQuery(this).parents('.botIcon').removeClass('showBotSubject');
		jQuery(this).parents('.botIcon').addClass('showMessenger');
	});
	
	/* Chatboat Code */
	$(document).on("submit", "#messenger", function(e) {
		e.preventDefault();

		var val = $("[name=msg]").val().toLowerCase();
		var mainval = $("[name=msg]").val();
		name = '';
		nowtime = new Date();
		nowhour = nowtime.getHours();

		function userMsg(msg) {
			$('.Messages_list').append('<div class="msg user"><span class="avtr"><figure style="background-image: url(https://lh3.googleusercontent.com/mCFhBsPRytVZaAFalccmqhzKCI3HTBOcP-EXMGHFICKHZ0oPH23ly7kdTUsYBW1ws7bkhwwakYNyRj_FtLKK2mQD5c8oqxiSYLbNKCkRGpuKAzXNt0sgnziqn3agyQ_HVuBfKjIUqA=w2400)"></figure></span><span class="responsText">' + mainval + '</span></div>');
		};
		function appendMsg(msg) {
			$('.Messages_list').append('<div class="msg"><span class="avtr"><figure style="background-image: url(https://lh3.googleusercontent.com/mCFhBsPRytVZaAFalccmqhzKCI3HTBOcP-EXMGHFICKHZ0oPH23ly7kdTUsYBW1ws7bkhwwakYNyRj_FtLKK2mQD5c8oqxiSYLbNKCkRGpuKAzXNt0sgnziqn3agyQ_HVuBfKjIUqA=w2400)"></figure></span><span class="responsText">' + msg + '</span></div>');
			$("[name='msg']").val("");
		};


		userMsg(mainval);
		if( val.indexOf("hello") > -1 || val.indexOf("hi") > -1 || val.indexOf("hey") > -1 || val.indexOf("good morning") > -1 || val.indexOf("good afternoon") > -1 || val.indexOf("good evening") > -1 || val.indexOf("good night") > -1 ) 
		{
			if(nowhour >= 12 && nowhour <= 16) 
			{
				appendMsg('good afternoon');
			} 
			else if(nowhour >= 10 && nowhour <= 12) 
			{
				appendMsg('hi');
			}
			else if(nowhour >= 0 && nowhour <= 10) 
			{
				appendMsg('good morning');
			} 
			else 
			{
				appendMsg('good evening');
			}
			appendMsg("what's you name?");
		} 
		else if( val.indexOf("problem") > -1 || val.indexOf("issue") > -1 || val.indexOf("trouble") > -1 || val.indexOf("worries") > -1 && val.indexOf("admin login") > -1) 
		{ 
			appendMsg("Login using admin credentials. Else Try contacting admin or changing password. ");
			appendMsg('Hope it was helpful. Any more queries? (Yes/No)');
		} 
		else if( val.indexOf("problem") > -1 || val.indexOf("issue") > -1 || val.indexOf("trouble") > -1 || val.indexOf("worries") > -1 && val.indexOf("employee") > -1) 
		{ 
			appendMsg("Sign in as an employee by clicking the button. You will be only able to display records and not make any changes to it.");
			appendMsg('Hope it was helpful. Any more queries? (Yes/No)');
		}
		else if( val.indexOf("when") > -1 && val.indexOf("jk paper") > -1 && val.indexOf("start") > -1 || val.indexOf("started") > -1 || val.indexOf("begin") > -1 || val.indexOf("began") > -1) 
		{ 
			appendMsg("Jk Paper Limited is a listed public company incorporated on 04 July, 1960.");
		}
		else if( val.indexOf("who") > -1  && val.indexOf("jk paper") && val.indexOf("founder") > -1 || val.indexOf("founded") > -1 || val.indexOf("started") > -1 || val.indexOf("begin") > -1 || val.indexOf("began") > -1) 
		{ 
			appendMsg("Lala Juggilal Singhania & Lala Kamlapat Singhania founded JK paper in 1918 along with cement, tyre, Software, FMCG and Dairy products");
		}
		else if( val.indexOf("who") > -1 && val.indexOf("current") > -1 && val.indexOf("chairperson") > -1 || val.indexOf("vc") > -1 || val.indexOf("md") > -1 ) 
		{ 
			appendMsg("Harsh Pati Singhania (born 1961) - Son of Bharat Hari Singhania. He is the current Vice Chairman & MD of JK Paper Limited.");
		}
		else if( val.indexOf("where") > -1 && val.indexOf("made") > -1 && val.indexOf("jk paper") > -1 ) 
		{ 
			appendMsg("JK Paper Ltd. has two large integrated paper manufacturing units - JK Paper Mills, Rayagada, Odisha and Central Pulp Mills, Songadh, Gujarat with a combined capacity of 4,55,000 TPA.");
		}
		else if( val.indexOf("where") > -1  && val.indexOf("jk paper") > -1 && val.indexOf("mills") > -1 && val.indexOf("located") > -1 ) 
		{ 
			appendMsg("JK Paper Ltd. has two large integrated paper manufacturing units - JK Paper Mills, Rayagada, Odisha and Central Pulp Mills, Songadh, Gujarat with a combined capacity of 4,55,000 TPA.");
		}
		else if( val.indexOf("where") > -1 && val.indexOf("world") > -1 && val.indexOf("mill") > -1 && val.indexOf("largest") > -1) 
		{ 
			appendMsg("International Paper Company is the world's largest paper manufacturing company based in Memphis, Tennessee in the United States of America. The company is 123 years old as it was founded in 1898.");
		}
		else if( val.indexOf("world") > -1 && val.indexOf("mill") > -1 && val.indexOf("largest") > -1) 
		{ 
			appendMsg("International Paper Company is the world's largest paper manufacturing company based in Memphis, Tennessee in the United States of America. The company is 123 years old as it was founded in 1898.");
		}
		else if( val.indexOf("can do") > -1 || val.indexOf("can you do") > -1 || val.indexOf("can you tell") > -1 ||  val.indexOf("can you show") > -1 || val.indexOf("can you help") > -1 || val.indexOf("your name") > -1) 
		{ 
			appendMsg("Hi there! Welcome to JK Paper - Creating Lasting Impressions. I am JaKe, JK Paper Intelligent Assistant, here to help you explore our products, answer your queries and much more. :)");
		}
		else if( val.indexOf("how are you") > -1 || val.indexOf("how you keeping") > -1 || val.indexOf("how you doing") > -1 ||  val.indexOf("sup") > -1|| val.indexOf("whats up") > -1) 
		{ 
			appendMsg("I am doing very well, hoping the same for you :)");
			appendMsg("How are you?");
		}
		else if(val.indexOf("i'm") > -1 || val.indexOf("i am") > -1 )
		{ 
			if(val.indexOf("not fine") > -1 || val.indexOf("not good") > -1 || val.indexOf("not well") > -1 ||  val.indexOf("not doing well") > -1|| val.indexOf("not okay") > -1 || val.indexOf("not alright") > -1)
			{
				appendMsg("Hope things get well. ");
				appendMsg("How can I help you?");
			}
			else if(val.indexOf("fine") > -1 || val.indexOf("good") > -1 || val.indexOf("well") > -1 ||  val.indexOf("awesome") > -1|| val.indexOf("okay") > -1 || val.indexOf("alright") > -1 )
			{ 
				appendMsg("Good to hear");
				appendMsg("How can I help you?");
			}
			else
			{
				if(val.indexOf("i am") > -1) 
				{
					name = val.replace("i am", "");
				}

				else if(val.indexOf("i'm") > -1) 
				{
					name = val.replace("i'm", "");
				}
				appendMsg("hi " + name + ", how can i help you?");
			}
		}
		else if( val.indexOf("yes") > -1) 
		{
			var nowtime = new Date();
			var nowhour = nowtime.getHours();
			appendMsg("How can i help you?");
		} 
		else if( val.indexOf("no") > -1) 
		{
			var nowtime = new Date();
			var nowhoue = nowtime.getHours();
			appendMsg("it's my pleasure that i could help you");
			saybye();
		} 
		else if( val.indexOf("bye") > -1 || val.indexOf("see you") > -1  || val.indexOf("see ya") > -1 || val.indexOf("tata") > -1 ) 
		{
			saybye();
		}
		else if( val.indexOf("my name is ") > -1 || val.indexOf("i am ") > -1 || val.indexOf("i'm ") > -1 || val.split(" ").length < 2 ) 
		{
			if(val.indexOf("my name is") > -1) {
				name = val.replace("my name is", "");
			}

			else if(val.indexOf("i am") > -1) {
				name = val.replace("i am", "");
			}

			else if(val.indexOf("i'm") > -1) {
				name = val.replace("i'm", "");
			}

			else {
				name = mainval;
			}
			appendMsg("hi " + name + ", how can i help you?");
		} 
		else if( val.indexOf("thank you") > -1 || val.indexOf("thanks") > -1  || val.indexOf("thenks") > -1 ) 
		{
			appendMsg("It was my pleasure to help you.");
		}
		else 
		{
			appendMsg("sorry i'm not able to understand what do you want to say");
		}

		function saybye() 
		{
			if (nowhour <= 10) 
			{
				appendMsg(" It was nice talking to you");
				appendMsg(" have nice day! :)");
			} 
			else if (nowhour >= 11 || nowhour <= 20) 
			{
				appendMsg(" It was nice talking to you");
				appendMsg(" bye!");
			} else 
			{
				appendMsg(" It was nice talking to you");
				appendMsg(" good night!");
			}
		}

		var lastMsg = $('.Messages_list').find('.msg').last().offset().top;
		$('.Messages').animate({scrollTop: lastMsg}, 'slow');
	});
	/* Chatboat Code */
})
</script>
